        <link rel="stylesheet" href="/css/responsive.css">
        <link rel="stylesheet" href="/css/style.css">

</head>

<body>

    <%- include("navbar") %>

    <div class="bg-img">
        <img src="/img/bg-img.jpg" alt="IIIT Una">
    </div>
    <div class="head">
    </div>
    <div class="login-card"><img class="profile-img-card" src="/img/Iiit-una-logo.png">
        <p class="profile-name-card">IIITU Admin Login</p>
        <form class="form-signin" method="POST" action="/admin-login"><span class="reauth-email"> </span>
            <input class="form-control" name="username" type="text" id="inputEmail" required="" placeholder="Username" autofocus="">
            <input class="form-control" name="password" type="password" id="inputPassword" required="" placeholder="Password">
                    <% if (message) { %>
                        <p style="color: red;">
                            <%= message %>
                        </p>
                        <% } %>
            <input type="submit" class="btn btn-primary btn-lg d-block btn-signin w-100" value="Sign in">
        </form>
    </div>
    <script src="/js/bootstrap.min.js"></script>
</body>

<%- include("footer") %>

